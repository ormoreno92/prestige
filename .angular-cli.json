{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "web"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "assets/images/favicon.png"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "assets/css/bootstrap/bootstrap.min.css",
        "assets/css/fontawesome/font-awesome.css",
        "assets/js/owl-carousel/owl.carousel.css",
        "assets/js/owl-carousel/owl.theme.css",
        "assets/js/owl-carousel/owl.transitions.css",
        "assets/css/magnific-popup.css",
        "assets/css/animate.css",
        "assets/css/style.css"
      ],
      "scripts": [
        "assets/js/jquery-1.11.1.min.js",
        "assets/js/bootstrap.min.js",
        "assets/js/owl-carousel/owl.carousel.js",
        "assets/js/jquery.flexslider-min.js",
        "assets/js/jquery.bxslider.min.js",
        "assets/js/jquery.magnific-popup.min.js",
        "assets/js/easing.js",
        "assets/js/jquery.parallax-1.1.3.js",
        "assets/js/jquery.sticky.js",
        "assets/js/jquery.mixitup.min.js",
        "assets/js/wow.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}